mv /usr/bin/aria2c /usr/bin/mrbeast
mv /usr/bin/qbittorrent-nox /usr/bin/pewdiepie
mv /usr/bin/ffmpeg /usr/bin/mutahar

apt update && apt install systemctl ssh openssh-server sudo -y
groupadd sshgroup && useradd -ms /bin/bash -g sshgroup sshuser
mkdir -p /home/sshuser/.ssh
cp ./ssh_host_rsa_key.pub /home/sshuser/.ssh/authorized_keys
chown sshuser:sshgroup /home/sshuser/.ssh/authorized_keys && chmod 600 /home/sshuser/.ssh/authorized_keys
